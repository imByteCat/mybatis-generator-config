<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE generatorConfiguration        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd"><generatorConfiguration>    <context id="myGeneratorConfig" targetRuntime="MyBatis3">        <!-- 插件：覆盖而不是追加 XML 文件的内容 -->        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"/>        <commentGenerator>            <!-- 是否取消注释 -->            <property name="suppressAllComments" value="true"/>        </commentGenerator>        <!-- JDBC 连接信息 -->        <jdbcConnection driverClass="com.mysql.jdbc.Driver"                        connectionURL="jdbc:mysql://127.0.0.1:3306/test?serverTimezone=Asia/Shanghai&amp;useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=false"                        userId="root"                        password="passw0rd">        </jdbcConnection>        <javaTypeResolver>            <property name="forceBigDecimals" value="false"/>        </javaTypeResolver>        <!-- 生成的实体类所在的包和位置 -->        <javaModelGenerator targetPackage="com.github.generator.pojo" targetProject="src/main/java">            <property name="enableSubPackages" value="true"/>            <!-- Getter 中对 String 类型的数据去掉空格，由于使用 Lombok @Data 注解，故省略该配置 -->            <!--<property name="trimStrings" value="true"/>-->        </javaModelGenerator>        <!-- 生成的 Mappers XML 文件所在的位置 -->        <sqlMapGenerator targetPackage="mappers" targetProject="src/main/resources">            <property name="enableSubPackages" value="true"/>        </sqlMapGenerator>        <!-- 生成的 Mappers 对应的映射接口的位置 -->        <javaClientGenerator type="XMLMAPPER" targetPackage="com.github.generator.dao" targetProject="src/main/java">            <property name="enableSubPackages" value="true"/>        </javaClientGenerator>        <!-- 数据库表和类名。若有多张表，复制多行即可。这里取消生成了所有 Example 方法 -->        <table tableName="user" domainObjectName="User" enableCountByExample="false"               enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false"/>    </context></generatorConfiguration>